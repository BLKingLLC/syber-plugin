<?xml version="1.0" encoding="UTF-8"?>
<MenuConfig>

	<Displayers>
		<Displayer name="DropDown"
			type="net.sf.navigator.displayer.DropDownMenuDisplayer" />
		<Displayer name="Simple"
			type="net.sf.navigator.displayer.SimpleMenuDisplayer" />
		<Displayer name="CoolMenu"
			type="net.sf.navigator.displayer.CoolMenuDisplayer" />
		<Displayer name="CoolMenu4"
			type="net.sf.navigator.displayer.CoolMenuDisplayer4" />
		<Displayer name="MenuForm"
			type="net.sf.navigator.example.PermissionsFormMenuDisplayer" />
		<Displayer name="ListMenu"
			type="net.sf.navigator.displayer.ListMenuDisplayer" />
		<Displayer name="TabbedMenu"
			type="net.sf.navigator.displayer.TabbedMenuDisplayer" />
		<Displayer name="Velocity"
			type="net.sf.navigator.displayer.VelocityMenuDisplayer" />
		<Displayer name="XtreeMenu"
			type="net.sf.navigator.displayer.XtreeMenuDisplayer" />
		<Displayer name="CSSListMenu"
			type="net.sf.navigator.displayer.CSSListMenuDisplayer" />
	</Displayers>
	
	<Menus>
		<Menu name="Home" title="home.title" page="/home" roles="ROLE_ADMIN,ROLE_USER" />
		<Menu name="Payroll" title="payroll.title" description="Payrolle Menu"
			page="/payroll/payroll" roles="ROLE_ADMIN,ROLE_USER" />

		<Menu name="EmployeeMenu" title="menu.employee" description="Account Provisioning"
			roles="ROLE_ADMIN,ROLE_USER" page="/employee/employees">
			
			<Item name="GoogleInit" title="menu.googleForm" description="Google User Form"
				page="/employee/googleInit" />
				
			<Item name="EmployeeBrowser" title="menu.employeeBrowser"
				description="Browse Ultipro Employees" page="/employee/employeesBrowser" />
				
			<Item name="EmployeeEdit" title="menu.ultiproUserForm" description="Ultipro User Form"
				page="/employee/ultiPro" />
			
			<Item name="SSOSynch" title="menu.ssoSynch" description="Synch SSO Users"
				page="/employee/ssoSynch" roles="ROLE_ADMIN" />
			
			<Item name="NewHireBrowser" title="menu.newHireBrowser"
				description="New Hire Provisioning" page="/employee/newHireBrowser" />
			
			<Item name="NewHireLogs" title="menu.newHireLogs" description="Provisioning Logs"
				page="/employee/newhirelogs" />
		</Menu>

		<Menu name="UserMenu" title="menu.user" description="User Menu"
			page="/userform" roles="ROLE_ADMIN,ROLE_USER" />

		<Menu name="AdminMenu" title="menu.admin" description="Admin Menu"
			roles="ROLE_ADMIN" page="/admin/users">
			<Item name="ViewUsers" title="menu.admin.users" page="/admin/users" />
			<Item name="ActiveUsers" title="home.activeUsers" page="/admin/activeUsers" />
			<Item name="ReloadContext" title="menu.admin.reload" page="/admin/reload" />
			<Item name="FileUpload" title="menu.selectFile" page="/fileupload" />
		</Menu>


		<Menu name="DebugMenu" title="menu.debug" description="Debug Menu"
			roles="ROLE_DEBUG">
			<Item name="SSOInfo" title="ssoInfo.title" description="Get a Users SSO Info"
				page="/employee/ssoInfo" />
		</Menu>


		<Menu name="Logout" title="user.logout" page="/logout"
			roles="ROLE_ADMIN,ROLE_USER" />

	</Menus>

</MenuConfig>
